      {
        "M": {
          "title": {
            "S": "üßë‚Äçüç≥ Recipe"
          },
          "type": {
            "S": "list"
          },
          "items": {
            "L": [
              {
                "S": "1 lb dried spaghetti"
              },
              {
                "S": "1 lb ground meat (beef or turkey)"
              },
              {
                "S": "3 tbsp olive oil"
              },
              {
                "S": "1 cup chopped onion"
              },
              {
                "S": "3 garlic cloves (1 tbsp), minced"
              },
              {
                "S": "2 tbsp tomato paste"
              },
              {
                "S": "1/2 tsp dried oregano"
              },
              {
                "S": "Pinch crushed red pepper flakes"
              },
              {
                "S": "1 cup water, broth, or red wine"
              },
              {
                "S": "1 (28oz) can crushed tomatoes"
              },
              {
                "S": "Salt & fresh ground black pepper"
              },
              {
                "S": "Handful fresh basil leaves, plus extra for serving"
              },
              {
                "S": "Grated parmesan cheese, for serving"
              },
              {
                "S": "Heat olive oil in a large pot over medium-high heat."
              },
              {
                "S": "Add ground meat and cook until browned (8 minutes), breaking up as it cooks."
              },
              {
                "S": "Add onions and cook until softened (5 minutes)."
              },
              {
                "S": "Stir in garlic, tomato paste, oregano, red pepper flakes; cook for 1 minute."
              },
              {
                "S": "Pour in water/broth/wine and scrape up any bits from the bottom."
              },
              {
                "S": "Add crushed tomatoes, 3/4 tsp salt, and black pepper; stir to combine."
              },
              {
                "S": "Simmer uncovered for 25 minutes, stirring and adjusting seasoning as needed."
              },
              {
                "S": "Meanwhile, cook pasta in salted water according to package (check early for doneness)."
              },
              {
                "S": "Remove sauce from heat, stir in fresh basil, then toss with cooked pasta."
              },
              {
                "S": "Let sit 1 minute to absorb, toss again, and serve with parmesan cheese."
              }
            ]
          }
        }
